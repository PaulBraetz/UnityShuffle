<Authorized>
	<CollapseFrame>
		<CollapseText>
			<CollapseChevron /><Localize Value="Change Email" />
		</CollapseText>
		<WhenNotCollapsed>
			<ChangeEmailPreset />
		</WhenNotCollapsed>
	</CollapseFrame>
	<CollapseFrame>
		<CollapseText>
			<CollapseChevron /><Localize Value="Change Password" />
		</CollapseText>
		<WhenNotCollapsed>
			<ChangePasswordPreset />
		</WhenNotCollapsed>
	</CollapseFrame>
	<AuthorizeView ForRight="@PBCommon.Configuration.Settings.LogReaderRight">
		<CollapseFrame>
			<CollapseText>
				<CollapseChevron/> <Localize Value="Logs"/>
			</CollapseText>
			<WhenNotCollapsed>
				<PBFrontend.UI.Meta.Logging.Logs/>
			</WhenNotCollapsed>
		</CollapseFrame>
	</AuthorizeView>
	<DeleteUser/>
	<LogoutPreset />
</Authorized>
<NotAuthorized>
	<CollapseFrame>
		<CollapseText>
			<CollapseChevron /><Localize Value="Register" />
		</CollapseText>
		<WhenNotCollapsed>
			<RegisterPreset />
		</WhenNotCollapsed>
	</CollapseFrame>
	<CollapseFrame>
		<CollapseText>
			<CollapseChevron /><Localize Value="Login" />
		</CollapseText>
		<WhenNotCollapsed>
			<LoginPreset />
		</WhenNotCollapsed>
	</CollapseFrame>
	<CollapseFrame>
		<CollapseText>
			<CollapseChevron /><Localize Value="Forgot Password" />
		</CollapseText>
		<WhenNotCollapsed>
			<ForgotPasswordPreset />
		</WhenNotCollapsed>
	</CollapseFrame>
</NotAuthorized>